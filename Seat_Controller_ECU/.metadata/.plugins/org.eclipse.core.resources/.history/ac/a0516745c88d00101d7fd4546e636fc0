#include "../MCAL/DIO/DIO.h"
#include "../LIB/BIT_MATH.h"
#include "../LIB/STD_TYPES.h"
#include "../MCAL/EEPROM/EEPROM.h"
#include "../MCAL/UART/UART.h"
#include <util/delay.h>
void main()
{
	UART_Init(9600, 8) ;

	u8 Received_Data = UART_RX();
	EEPROM_WRITE(0x70, Received_Data);
	_delay_ms(500) ;
	while (1)
	{
	}
}

